(ns pile.test.equality
    (:refer pile.test pile.test.util))
           
(import (java.util HashMap ArrayList HashSet))

(defn jmap [k v]
    (let [m (new HashMap)]
        (do 
            (.put m k v)
            m)))

(def java-map (new HashMap))
(.put java-map :a :b)

(def pile-map {:a :b})

(test= ["Equals" "Map"] 
    pile-map java-map)
(test= ["Equals" "Map" "Reverse"] 
    java-map pile-map)

(test= ["Equals" "MapKey"] 
    (hash-map java-map 12)
    (hash-map pile-map 12))
    
(test= ["Equals" "PileBase" "MapKey" "Get" "LhsJava" "RhsJava"]
    12 (get (hash-map java-map 12) java-map))
(test= ["Equals" "PileBase" "MapKey" "Get" "LhsJava" "RhsPile"]
    12 (get (hash-map java-map 12) pile-map))
(test= ["Equals" "PileBase" "MapKey" "Get" "LhsPile" "RhsJava"]
    12 (get (hash-map pile-map 12) java-map))
(test= ["Equals" "PileBase" "MapKey" "Get" "LhsPile" "RhsPile"]
    12 (get (hash-map pile-map 12) pile-map))    
    
(test= ["Equals" "JavaBase" "MapKey" "Get" "LhsJava" "RhsJava"]
    12 (get (jmap java-map 12) java-map))
(test= ["Equals" "JavaBase" "MapKey" "Get" "LhsJava" "RhsPile"]
    12 (get (jmap java-map 12) pile-map))
(test= ["Equals" "JavaBase" "MapKey" "Get" "LhsPile" "RhsJava"]
    12 (get (jmap pile-map 12) java-map))
(test= ["Equals" "JavaBase" "MapKey" "Get" "LhsPile" "RhsPile"]
    12 (get (jmap pile-map 12) pile-map))   

